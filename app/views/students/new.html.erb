<!-- Create your form here -->
Student Form
<!-- rails form_with will assume that I want to POST to students_path -->
<%= form_tag students_path do %>
<!-- form for will try to populate with attributes found on @student instance -->
 
  <%= text_field_tag :first_name, id: :first_name %>
  <br>

  <%= text_field_tag :last_name, id: :last_name %>

<br>
  <%= submit_tag("Submit Student")  %>
<% end %>